<Window x:Class="RestaurantManagement.PastReceipts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RestaurantManagement"
        mc:Ignorable="d"
        Title="PastReceipts" Height="600" Width="600" Background="Aquamarine">

    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel x:Name="myStackPanelPastReceipts" HorizontalAlignment="Center" Background="Aquamarine">
            <Button Width="100" Height="50" FontFamily="Georgia" FontSize="20" Click="Back_Click" FontWeight="Bold" Margin="5, 5, 5, 5" HorizontalAlignment="Center" Content="Go Back"/>
        </StackPanel>
    </ScrollViewer>
</Window>
